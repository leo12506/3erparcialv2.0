#include <iostream>

using namespace std;

int main() {
    char caracter;

    std::cout<<"Ingrese un carácter: ";
    cin>>caracter;

    switch (caracter) {
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':
            cout<<"El carácter ingresado es una vocal.\n";
            break;
        default:
            cout<<"El carácter ingresado no es una vocal.\n";
            break;
    }

    return 0;
}
