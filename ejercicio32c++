#include <iostream>
using namespace std;
int main(){
	int num [5]; 
	int i,numero;
	for(int i=0;i<5;i++){
	cout<<"Digite un numero para la posicion "<<i+1<<endl;
	cin>>numero;
	num[i]=numero;
	}
	for(int i=0;i<5;i++){
        cout<<"El dato en la posicion es "<<i+1<<" es: "<<num[i]<<endl; 
	}
	return 0;
}
