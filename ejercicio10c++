#include <iostream>
using namespace std;
int main()
{
	int n,n2,resul;
	cout<<"ingresa un numero para sumar ";
	cin>>n;
	cout<<"ingresa el segundo numero ";
	cin>>n2;
	if(n>0 && n2>0)
	{
	    resul=n+n2;
	    cout<<"tu resultado es:"<<resul;
	}else if(n=0)
	{
	    cout<<"un numero tiene cero vuelve a intentar";
	}else{
	    cout<<"un numero tiene cero vulve a intentar";
	}
	return 0;
}
