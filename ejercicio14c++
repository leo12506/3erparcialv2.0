#include <iostream>
using namespace std;

int main() {
    int num, menor,mayor; 

   cout<<"Ingrese 2 numeros enteros:\n";
    cin>>num;
    menor = num;
    int igual=num;

    for (int i = 1; i < 2; i++) 
    {
        cin>>num;

        if(num < menor) 
        {
            menor = num;
        }
        else if(num>menor)
        {
            mayor=num;
        }
        else
        {
            num=igual;
            cout<<"El numero igual es: "<<igual<<endl;
        }
    }

    cout<<"El numero menor es:"<< menor<<endl;
    cout<<"El numero mayor es: "<<mayor<<endl;
    return 0;
}
