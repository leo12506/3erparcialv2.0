include<iostream>
using namespace std;
int main(){
int acum,t,num;
acum=0;
cout<<"digite el tamano de vector ";
cin>>t;
int vector[t];
for(int i=1;i<=t;i++){
cout<<"digite un numero ";
cin>>num;
vector[i]=num;
acum=acum+vector[1];
}
for(int i=1;i<=t;i++){
cout<<"la suma de "<<vector[i]<<endl;
}
	cout<<"es "<<acum;
	return 0;
}
