#include <iostream>
using namespace std;
int main()
{
	int n,numero;
	cout<<"Digite el tamano del arreglo: ";
	cin>>n;
	 int num[n];
	for(int i=1;i<=n;i++){
	cout<<"Digite un numero para la posicion \n"<<i<<":";
	cin>>num[i];
	}

	for(int i=1;i<=n;i++){
       cout<<"\n El dato en la posicion "<<i<<" es: "<<num[i]<<endl;
	}
	return 0;
}
