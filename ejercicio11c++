#include <iostream>
using namespace std;
int main()
{
	int hora;
	cout<<"Ingresa la hora: ";
	cin>>hora;
	if(hora>=0 && hora<=12)
	{
	    cout<<"buenos dias";
	}
	else if (hora>12 && hora<=18)
	{
	    cout<<"buenas tardes";
	}
	else if(hora>19 && hora<=24 )
	{
	    cout<<"buenas noches";
	}
	return 0;
}
