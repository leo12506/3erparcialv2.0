#include <iostream>
using namespace std;
int main (){
    string arreglo1[2];
     int arreglo2[3];
    arreglo1[0]="fernando";
    arreglo1[1]="oscar";
    
    arreglo2[0]=74;
    arreglo2[1]=56;
    arreglo2[2]=10;
    cout<<"arreglo1"<<endl;
    for(int i=0;i<2;i++){
        cout<<"la posicion"<<i<<"es "<<arreglo1[i]<<endl;
    }
    cout<<"arreglo2"<<endl;
    for(int i=0;i<3;i++){
        cout<<"La posicion "<<i<<"es "<<arreglo2[i]<<endl;
    }
 return 0;
}
